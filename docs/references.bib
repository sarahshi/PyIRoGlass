@article{Faaketal2013,
	abstract = {The temperature-sensitive exchange of Mg between plagioclase (Pl) and clinopyroxene (Cpx) has been studied experimentally, accounting for different anorthite-contents in plagioclase (XAn) and various silica activities (aSiO2) in the system. The partitioning of Mg between plagioclase and clinopyroxene was determined over a temperature range of 1100--1200$\,^{\circ}$C, using plagioclase single crystals of different compositions (XAn=0.5--0.8), surrounded by different clinopyroxene-bearing matrix powders to account for different silica activities from 0.55 to 1.0. The experimental design also allows the diffusivity (DMgPl) of Mg in plagioclase under these conditions to be determined. Both KMgPl/Cpx (defined as KMgPl/Cpx=CMgPl/CMgCpx) and DMgPl decrease with temperature and increase with aSiO2. Isothermal data for different XAn in plagioclase show a linear increase of lnKMgPl/Cpx with increasing XAn, but DMgPl appears to be insensitive to XAn. The partitioning data allow a new geothermometer to be calibrated, which may be widely applicable to terrestrial and extraterrestrial rocks where plagioclase and clinopyroxene coexist: T[K]=(-9219+2034XAn)/(lnKMgPl/Cpx-1.6-lnaSiO2). Application of this geothermometer to experimental data from this study reproduces the experimental temperatures within $\pm$20$\,^{\circ}$C. Diffusion of Mg in plagioclase is described by DMgPl[m2s-1]=1.2510-4[m2s-1]exp(-320,924[J mol-1]/(RT))(aSiO2)2.6.},
	author = {Kathrin Faak and Sumit Chakraborty and Laurence A. Coogan},
	date-added = {2022-06-21 11:50:24 +0100},
	date-modified = {2022-09-10 15:15:22 -0400},
	doi = {https://doi.org/10.1016/j.gca.2013.05.009},
	issn = {0016-7037},
	journal = {Geochimica et Cosmochimica Acta},
	pages = {195-217},
	title = {{M}g in plagioclase: {E}xperimental calibration of a new geothermometer and diffusion coefficients},
	volume = {123},
	year = {2013},
	bdsk-url-1 = {https://www.sciencedirect.com/science/article/pii/S0016703713002731},
	bdsk-url-2 = {https://doi.org/10.1016/j.gca.2013.05.009}}

